extends ../layout/layout.pug

block variable
    -var path = '../';



block content
    // begin firstscreen style="background-image: url('../img/project.webp')"
    section#firstscreen.firstscreen.section
        .firstscreen__img.img.desktop
            img(src="../img/firstscreen_bg.webp", alt="alt")
        .firstscreen__img.img.mobile
            img(src="../img/firstscreen_bg_m.webp", alt="alt")
        .container_center
            .firstscreen__content
                h1.section__title
                    span Виллы 
                    span и таунхаусы Дубая

                    small Лучшие проекты для жизни и инвестирования
                .firstscreen__btn
                    a.btn(href="#") Начать подбор



    // end firstscreen

    // begin choose
    section#choose.choose.section
        .container_center
            .choose__caption.desktop
                h2.section__title(data-aos="fade-up")
                    span Выбирайте,<br>что подойдет<br>именно вам
                .choose__label
                    span 6 готовых подборок проектов
            .choose__content
                .choose__caption.mobile
                    h2.section__title
                        span Выбирайте,<br>что подойдет<br>именно вам
                    .choose__label
                        span 6 готовых подборок проектов
                .choose__grid
                    each item in [1,2,3,4,5,6]
                        a(href="#modalChoose").choose__item.img.show_js
                            img(data-ru="../img/select_"+item+".webp" data-en="../img/no_img.png" src="" alt="alt")

    // end choose

    // begin apartments
    section#apartments.apartments.section
        .container_center
            .apartments__filter
                .filter#filter
                    .filter__row
                        .filter__col
                            .filter__label
                                span Цена, $
                            .filter__price
                                .filter__number.filter__number_from

                                    //- input.number_shipments_js(type="number" placeholder="0" min="0" max="1000" step="1")
                                    label
                                        span от
                                    input(class="ranks_js" type="text" placeholder="100 000")

                                .filter__number.filter__number_before
                                    label 
                                        span до
                                    input(class="ranks_js" type="text" placeholder="10 000 000")


                        .filter__col
                            .filter__label
                                span Количество спален
                            ul.filter__list
                                each item in ['2','3','4','5+']
                                    li.filter__checkbox
                                        input(type="checkbox" value="apartments-"+item+"" id="apartments_"+item+"")
                                        label(for="apartments_"+item+"")
                                            span=item

                        .filter__col
                            ul.filter__more            
                                li.filter__checkbox.filter__checkbox_more
                                    input(type="checkbox" value="coastline" id="coastline")
                                    label(for="coastline")
                                        span 1 береговая линия
                                li.filter__checkbox.filter__checkbox_more
                                    input(type="checkbox" value="pool" id="pool")
                                    label(for="pool")
                                        span Личный бассейн

                        .filter__col
                            .filter__select
                                select.select(name="select" data-placeholder="По популярности")
                                    option(value="")
                                    option(value="default" selected="selected") По популярности
                                    option(value="top") По возрастанию
                                    option(value="bottom") По убыванию

            .apartments__list

                each item in ['Malta at Damac Lagoons', 'Malta at Damac', ]

                    .apartments__item
                        .apartments__gallery

                            .apartments__img.img
                                a(href="../img/slide_1.jpg" data-fancybox data-type="image")
                                    img(src="../img/slide_1.jpg" loading="lazy" alt="img")

                            .apartments__slider
                                .swiper.apartments_swiper_js
                                    .swiper-wrapper
                                        each item in [2,3,4,2,3,4]
                                            .swiper-slide
                                                .apartments__img.img
                                                    a(href="../img/slide_"+item+".jpg" data-fancybox data-type="image")
                                                        img(src="../img/slide_"+item+".jpg" loading="lazy" alt="img")
                                .apartments__arrows
                                    i.icon_arrow_left
                                    i.icon_arrow_right

                        .apartments__content
                            .apartments__header
                                span(data-title=""+item+"" data-subject-title=""+item+"")!=item
                            .apartments__main
                                .details
                                    .details__item
                                        span IV 2024
                                    .details__item.details__item_location
                                        span Damac Lagoons

                                p Элитная недвижимость в Дубаи — это возможность приобрести действительно эксклюзивное жилье, которое станет настоящим украшением вашего дома или офиса. Находясь в центре Дубая, вы имеете доступ ко всем необходимым для комфортной жизни услугам, включая рестораны, магазины, развлекательные центры и многое другое. 

                                .digits
                                    .digits__item
                                        i.icon_area
                                        span 350-450  
                                        span &nbsp;м
                                            sup 2
                                        .apartments__reverse
                                            span 1234-1234 
                                            span фт 
                                                sup 2

                                    .digits__item
                                        .digits__img.img
                                            img(src="../img/bed.svg", alt="alt")
                                        span 3-5 спален

                            .apartments__footer
                                .apartments__price
                                    span от $
                                    span 2 500 000
                                    .apartments__reverse
                                        span от 
                                        span 9 500 000 &nbsp;
                                        span AED
                                .apartments__btn
                                    a.btn.btn_transparent.show_js(href="#aboutProject") Узнать подробнее



    // end apartments

    // begin partners
    section#partners.partners.section
        .container_center
            .partners__bg
    // end partners

    // begin payment
    section#payment.payment.section
        .container_center
            .payment__caption.mobile
                h2.section__title.section__title_sm
                    span Для покупки<br> не нужно вносить<br> всю сумму сразу
                .payment__desc
                    span На все проекты есть беспроцентная рассрочка на весь срок строительства с комфортным планом платежей
                .payment__label
                    span Пример плана платежей:

            .payment__content
                .payment__caption.desktop
                    h2.section__title.section__title_sm
                        span Для покупки<br> не нужно вносить<br> всю сумму сразу
                    .payment__desc
                        span На все проекты есть беспроцентная рассрочка на весь срок строительства с комфортным планом платежей

                .payment__wrap

                    .payment__list
                        each item in [{delay: '400', number:'20%', name:'1-й платёж',bottom:'при бронировании' }, {delay: '350', number:'10%', name:'2-й платёж',bottom:'3 месяца' }, {delay: '300', number:'10%', name:'3-й платёж',bottom:'9 месяцев' }, {delay: '250', number:'10%', name:'4-й платёж',bottom:'12 месяцев' }, {delay: '200', number:'10%', name:'5-й платёж',bottom:'18 месяцев' }, {delay: '150', number:'10%', name:'6-й платёж',bottom:'24 месяца' }, {delay: '100', number:'10%', name:'7-й платёж',bottom:'30 месяцев' }, {delay: '50', number:'20%', name:'последний платёж',bottom:'при сдаче' }, ]


                            .payment__item
                                .payment__top(data-aos="fade-up" data-aos-delay=""+item.delay+"")
                                    .payment__info
                                        span=item.number
                                        p=item.name
                                .payment__bottom(data-aos="fade-up" data-aos-delay="700")
                                    span=item.bottom

                    .payment__label.desktop 
                        span Пример плана платежей

            .payment__more
                p рассрочка до <span>4</span> лет
                p <span>0%</span> переплат


    // end payment

    // begin relevant
    section#relevant.relevant.section
        .container_center
            h2.section__title
                span В нашей базе более 
                span 30 актуальных 
                span проектов
                    small всех вилл<br>и таунхаусов Дубая
            .relevant__wrap
                .relevant__content
                    .relevant__title
                        span Не знаете какой<br> подойдет именно вам?
                    .relevant__text
                        span Пройдите короткий опрос, укажите интересующие параметры, и эксперт проекта индивидуально подберет проекты под ваш запрос и отправит подборку мессенджер или на почту.
                    .relevant__btn
                        a.btn(href="#") Пройти опрос и получить подборку
                    .relevant__label.mobile
                        span 1-2 минуты время прохождения

                .relevant__preview
                    .relevant__img.img
                        img(data-ru="../img/relevant_img.webp" data-en="../img/no_img.png" src="", alt="alt")
                    .hint.desktop(data-aos="fade-left" data-aos-delay="300")
                        .hint__content
                            span 1-2 минуты время прохождения



    // end relevant

    // begin experts
    section#experts.experts.section
        .container_center
            h2.section__title(data-aos="fade-up")
                span Что говорят эксперты рынка
            .experts__label
                .experts__flag.img
                    img(src="../img/UAE.png", alt="alt")
                span недвижимости Дубая

        .experts__slider
            .swiper.experts_swiper_js
                .swiper-wrapper
                    each item in ['', '', '', ]

                        .swiper-slide
                            .experts__content
                                .experts__top
                                    .experts__title
                                        span Почему формат вилл и таунхаусов становится более популярен?
                                    .experts__text
                                        p Элитная недвижимость в Дубаи — это возможность приобрести действительно эксклюзивное жилье, которое станет настоящим украшением вашего дома или офиса. Вы сможете любоваться видами на море, находясь при этом в тишине и покое. Находясь в центре Дубая, вы имеете доступ ко всем необходимым для комфортной жизни услугам, включая рестораны, магазины и многое другое. 
                                        p Все элитные дома в Дубае отличаются безупречным качеством, а также высоким уровнем комфорта.

                                    .experts__btn
                                        a.btn.btn_transparent(href="#") Читать статью полностью

                                .experts__bottom
                                    .experts__manager
                                        .experts__img.img
                                            img(src="../img/manager.webp", alt="alt")
                                        .experts__info
                                            i Анна
                                            span эксперт<br> недвижимости<br> в Дубае

                    .swiper-slide
                        .experts__content.experts__content_last
                            .experts__last
                                .experts__title
                                    span Узнайте подробнее<br>о виллах в Дубае<br>в нашем блоге
                                .experts__btn
                                    a.btn(href="#") Перейти в блог
                            .experts__masc.img.desktop
                                img(src="../img/experts_masc.webp", alt="alt")


                .experts__arrows.desktop
                    i.icon_arrow_left_lg
                    i.icon_arrow_right_lg


                .swiper_label.d_left.swiper_label_js
                    span тяните


    // end experts

    // begin catalog
    section#catalog.catalog.section
        .container_center
            .catalog__content
                .catalog__caption

                    h2.section__title.desktop
                        span(data-aos="fade-right" data-aos-delay="0") Скачайте
                        span(data-aos="fade-right" data-aos-delay="100") полный
                        span(data-aos="fade-right" data-aos-delay="200")(data-pdf="pdf • 12мб") каталог
                    h2.section__title.mobile
                        span Скачайте полный каталог
                    .catalog__desc
                        span всех вилл и таунхаусов Дубая

                .catalog__form
                    form.form(action="send.php")
                        input(type="hidden" name="subject" value="форма в футоре")
                        input(type="hidden" name="notspam" value="")

                        .form__row
                            input(type="text" placeholder="Имя")

                        .form__row
                            input.tel(type="text" required placeholder="+")

                        .form__row
                            button.btn.btn_lg(type="submit") Получить каталог

                        .form__row
                            .form__text
                                span Нажимая на кнопку вы соглашаетесь с условиями обработки данных и политикой конфиденциальности
                .catalog__bg(style="background-image: url('../img/catalog.webp')")
                    .hint(data-aos="fade-up" data-aos-delay="400")
                        .hint__content
                            span 35 проектов с планировками, ценами и описаниями



    // end catalog

    // begin feedback
    section#feedback.feedback.section
        .container_center
            h2.section__title(data-aos="fade-up")
                span Не нашли то,<br>что искали?
            .feedback__content
                span Напишите нам и мы ответим на ваш вопрос
                .feedback__action
                    a.btn.btn_whatsapp(target="_blank" href="https://wa.me/") Whatsapp
                    a.btn.btn_telegram(target="_blank" href="https://t.me/") Telegram

    // end feedback
